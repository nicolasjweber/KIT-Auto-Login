{
  "manifest_version": 2,
  "name": "KIT Auto-Login",
  "version": "8.10.7",
  "description": "Clicks through the KIT ILIAS & CAS Campus login pages automatically.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ilias.studium.kit.edu/login*",
        "*://*.campus.studium.kit.edu/*",
        "*://*.ilias.studium.kit.edu/ilias.php*",
        "*://*.idp.scc.kit.edu/idp/profile*"
      ],
      "js": [
        "login.js"
      ]
    }
  ]
}